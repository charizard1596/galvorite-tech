
{
  "multipart": [
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "galvorite:block/wire",
        "x": 270
      }
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "galvorite:block/wire",
        "x": 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "galvorite:block/wire"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "galvorite:block/wire",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "galvorite:block/wire",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "galvorite:block/wire",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "galvorite:block/wire_side",
        "y": 90
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "galvorite:block/wire_side",
        "x": 270
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "galvorite:block/wire_side",
        "x": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "galvorite:block/wire_side",
        "y": 270
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "galvorite:block/wire_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "galvorite:block/wire_side",
        "y": 180,
        "uvlock": true
      }
    }
  ]
}

